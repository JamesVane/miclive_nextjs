{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": ["./*"],
			"@desk/*": ["./desktopFolder/*"],
			"@mobi/*": ["./mobile_folder/*"],
			"@styl/*": ["./styles/*"]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"app/muiprovider.ts",
		"useWatchableEffect/propWatcher.js",
		"useWatchableEffect/utils.js",
		"desktopFolder/PublicProfilePage",
		"desktopFolder/PublicProfilePageContainer"
	],
	"exclude": ["node_modules"]
}
